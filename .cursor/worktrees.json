{
  "setup-worktree": [
    "echo 'Configuring worktree for parallel agents...'",
    "rsync -a --include='*/' --include='.env*' --exclude='*' \"$(git worktree list | head -1 | awk '{print $1}')/\" .",
    "git submodule update --init --recursive",
    "cd frontend",
    "npm install",
    "echo 'Worktree setup complete!'"
  ]
}
